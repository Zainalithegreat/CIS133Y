<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="ttk.Frame" id="main" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="Topic" named="True">
        <property name="anchor">n</property>
        <property name="compound">top</property>
        <property name="cursor">arrow</property>
        <property name="font">{Arial} 16 {bold}</property>
        <property name="text" translatable="yes">Bitcoin Calculator Project</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">0</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Frame" id="AboutProject" named="True">
        <property name="height">200</property>
        <property name="width">200</property>
        <layout manager="grid">
          <property name="column">0</property>
          <property name="row">1</property>
        </layout>
        <child>
          <object class="tk.Message" id="message1">
            <property name="font">TkDefaultFont</property>
            <property name="text" translatable="yes">Featuring an About tab and a Bitcoin Calculator. 
By Zain Ali
and Ben duChalard</property>
            <layout manager="pack">
              <property name="side">top</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
  </object>
</interface>
