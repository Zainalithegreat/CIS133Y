<?xml version='1.0' encoding='utf-8'?>
<interface version="1.3">
  <object class="ttk.Frame" id="main" named="True">
    <property name="height">200</property>
    <property name="width">200</property>
    <layout manager="grid">
      <property name="column">0</property>
      <property name="row">0</property>
    </layout>
    <child>
      <object class="ttk.Label" id="Answer" named="True">
        <property name="font">{Arial} 16 {bold}</property>
        <property name="justify">center</property>
        <property name="text" translatable="yes">Answer</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
    <child>
      <object class="ttk.Label" id="message" named="True">
        <property name="anchor">n</property>
        <property name="padding">100</property>
        <property name="textvariable">string:text</property>
        <layout manager="pack">
          <property name="side">top</property>
        </layout>
      </object>
    </child>
  </object>
</interface>
